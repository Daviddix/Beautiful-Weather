{
    "manifest_version": 3,
    "name": "Weather Extension",
    "description": "_blank is a chrome extension that makes every link you click open in a new tab",
    "version": "1.0.0",
    "action": { "default_popup": "index.html" },
      "permissions": ["storage", "activeTab", "tabs"],
    "icons": {
      "16": "icons/icon16.png", 
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }